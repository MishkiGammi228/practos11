        <Button x:Name="Exite" Content=" X " HorizontalAlignment="Left" Margin="231,8,0,0" VerticalAlignment="Top" Click="Exite_Click"/>
        <Button x:Name="Info" Content=" ? " HorizontalAlignment="Left" Margin="203,8,0,0" VerticalAlignment="Top" Click="Info_Click"/>

